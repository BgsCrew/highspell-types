{
  "name": "highspell-sdk-monorepo",
  "private": true,
  "scripts": {
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "build": "pnpm -r build",
    "build:types": "pnpm --filter @bgscrew/highspell-types build",
    "build:all": "pnpm build:types && pnpm -r build",
    "test:all": "pnpm build:types && pnpm -r test",
    "check": "pnpm -r check",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
