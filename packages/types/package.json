{
  "name": "@bgscrew/highspell-types",
  "version": "1.1.1",
  "description": "TypeScript definitions and generation tools for the HighSpell game client.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node scripts/generate-types.js",
    "test": "echo 'No tests defined' && exit 0",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "check": "pnpm lint && pnpm format --check && pnpm type-check"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./GameAction": {
      "import": "./dist/GameAction.js"
    },
    "./mappings": {
      "import": "./dist/mappings.js",
      "require": "./dist/mappings.js"
    },
    "./dist/mappings": {
      "import": "./dist/mappings.js",
      "require": "./dist/mappings.js"
    }
  },
  "dependencies": {
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bgscrew/highspell-types.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
